<div class="product-card">
  <img
    src="/images/products/<%= product.category %>/<%= product.imgName %>"
    alt=""
  />
  <p class="product-name"><%= product.name %></p>
  <p class="product-price"><%= product.price %></p>
  <div>
    <button>Learn more</button>
    <button>Buy now</button>
  </div>
</div>
